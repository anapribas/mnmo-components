{
  "name": "mnmo-components",
  "version": "0.1.1",
  "description": "A collection of UI components",
  "main": "./lib/index.js",
  "scripts": {
    "test": "echo type npm run --list for a list of tasks available",
    "start": "npm run www:serve",
    "outdated": "npm outdated --depth=0",
    "lint": "eslint ./lib",
    "clean": "rm -rf ./dist",
    "font:clean": "rm -rf ./assets/fonts",
    "font:mkdir": "mkdir -p ./assets/fonts",
    "font:mkdirwin": "mkdirp ./assets/fonts",
    "font:download:roboto": "goog-webfont-dl --all -f Roboto",
    "font:download:custom-icons": "./bin/downloadCustomEntypo.sh",
    "font:download:custom-iconswin": "node ./bin/downloadCustomEntypo.js",
    "font:mv:roboto": "mv Roboto* ./assets/fonts",
    "npm:clean": "rm -rf ./dist/npm",
    "npm:mkdir": "mkdir -p ./dist/npm/es6 && mkdir -p ./dist/npm/lib/themes/mnmo/styles",
    "npm:mkdirwin": "mkdirp ./dist/npm/es6 && mkdirp ./dist/npm/lib/themes/mnmo/styles",
    "npm:transpile": "babel -d ./dist/npm/lib -m common --optional spec.protoToAssign ./lib",
    "postnpm:transpile": "npm run npm:edit:jsx-references",
    "npm:edit:jsx-references": "find ./dist/npm/lib -type f -name \"*.js\" -print0 | xargs -0 sed -i.bak \"s@\\.jsx@@g\"",
    "postnpm:edit:jsx-references": "find ./dist/npm/lib -type f -name \"*.bak\" -print0 | xargs -0 rm -fv",
    "npm:cp:package.json": "cp -f ./package.json ./dist/npm/.",
    "npm:cp:es6-lib": "cp -R ./lib ./dist/npm/es6/.",
    "npm:cp:css": "cp -f ./lib/themes/mnmo/styles/*.css ./dist/npm/lib/themes/mnmo/styles/.",
    "www:clean": "rm -rf ./dist/www",
    "www:mkdir": "mkdir -p ./dist/www/js && mkdir -p ./dist/www/css && mkdir -p ./dist/www/fonts && mkdir -p ./dist/www/img",
    "www:js:polyfill:es5-shim": "cp node_modules/es5-shim/es5-shim.min.js ./dist/www/js/.",
    "www:js:polyfill:es5-sham": "cp node_modules/es5-shim/es5-sham.min.js ./dist/www/js/.",
    "www:js:polyfill:console": "cp node_modules/console-polyfill/index.js ./dist/www/js/console-polyfill.js",
    "www:js:vendors": "browserify -r react -o ./dist/www/js/vendors.js",
    "www:js:app": "browserify -t babelify -x react -e ./app.js -o ./dist/www/js/app.js",
    "www:html:index": "babel-node ./bin/createPage.js > ./dist/www/index.html",
    "www:css:normalize": "cp -f ./node_modules/normalize.css/normalize.css ./dist/www/css/.",
    "www:css:mnmo:typography": "cp -f ./lib/themes/mnmo/styles/typography.css ./dist/www/css/.",
    "www:img:custom": "cp ./assets/custom/dev/bg01.jpg ./dist/www/img/.",
    "www:assets:fonts": "cp -f ./assets/fonts/Entypo/* ./dist/www/fonts/. && cp -f ./assets/fonts/Roboto/Roboto-Thin.ttf ./assets/fonts/Roboto/Roboto-Thin.woff ./assets/fonts/Roboto/Roboto-Light.ttf ./assets/fonts/Roboto/Roboto-Light.woff ./assets/fonts/Roboto/Roboto-Bold.ttf ./assets/fonts/Roboto/Roboto-Bold.woff ./assets/fonts/Roboto/Roboto.eot  ./assets/fonts/Roboto/Roboto.svg  ./assets/fonts/Roboto/Roboto.ttf  ./assets/fonts/Roboto/Roboto.woff ./dist/www/fonts/.",
    "www:serve": "babel-node server.js",
    "npm:cp": "npm run npm:cp:package.json && npm run npm:cp:es6-lib && npm run npm:cp:css",
    "prenpm": "npm run npm:clean && npm run npm:mkdir",
    "npm": "npm run npm:transpile & npm run npm:cp",
    "prenpmwin": "npm run npm:clean && npm run npm:mkdirwin",
    "npmwin": "npm run npm:transpile & npm run npm:cp",
    "font": "npm run font:clean && npm run font:mkdir && npm run font:download:roboto && npm run font:mv:roboto && npm run font:download:custom-icons",
    "fontwin": "npm run font:clean && npm run font:mkdirwin && npm run font:download:roboto && npm run font:mv:roboto && npm run font:download:custom-iconswin",
    "www:js:polyfill": "npm run www:js:polyfill:es5-shim && npm run www:js:polyfill:es5-sham && npm run www:js:polyfill:console",
    "www:js": "npm run www:js:vendors & npm run www:js:app && npm run www:js:polyfill",
    "www:html": "npm run www:html:index",
    "www:css": "npm run www:css:normalize && npm run www:css:mnmo:typography",
    "www:assets": "npm run www:img:custom && npm run www:assets:fonts",
    "prewww": "npm run www:clean && npm run www:mkdir",
    "www": "npm run www:js & npm run www:html & npm run www:assets && npm run www:css",
    "dev": "watchify -t babelify -x react -e ./app.js -o ./dist/www/js/app.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fczuardi/mnmo-components.git"
  },
  "keywords": [
    "react",
    "components",
    "widgets",
    "mobile",
    "ui",
    "mnmo"
  ],
  "author": "Fabricio C Zuardi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fczuardi/mnmo-components/issues"
  },
  "homepage": "https://github.com/fczuardi/mnmo-components",
  "private": false,
  "peerDependencies": {
    "react": "^0.13.0"
  },
  "devDependencies": {
    "babel": "^5.6.7",
    "babelify": "^6.1.1",
    "browserify": "^11.0.1",
    "console-polyfill": "^0.2.3",
    "download-file": "^0.1.5",
    "es5-shim": "^4.1.7",
    "eslint": "^1.3.1",
    "goog-webfont-dl": "^0.1.1",
    "js-beautify": "^1.6.4",
    "koa": "^1.2.4",
    "koa-static": "^1.4.9",
    "lodash": "^3.9.1",
    "normalize.css": "^3.0.3",
    "object-assign": "^4.0.1",
    "react": "^0.13.3",
    "shelljs": "^0.7.5",
    "watchify": "^3.2.3"
  }
}
